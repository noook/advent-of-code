{
   "compilerOptions": {
      "emitDecoratorMetadata": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "lib": [
         "es5",
         "es6"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "target": "es6",
      "types": [
         "node"
      ],
      "typeRoots": [
         "./node_modules/@types"
      ]
   }
}